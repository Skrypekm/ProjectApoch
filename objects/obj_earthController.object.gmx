        <arguments>
          <argument>
            <kind>1</kind>
            <string>///player control
if obj_player.control
{    
    with (obj_player)
    {
        var testInt
        //move left and right
        if keyboard_check(ord("A"))     //LEFT MOVEMENT ---------------------------------------------------------------------------------------------------------
        {
            image_xscale = -abs(image_xscale);
            
            testInt = instance_place(x - Speed, y, Solid)
            if testInt == noone
            {
                //if free to move, move the player
                x -= Speed;
                    
                //parallax effect left
                var cameraX = view_xview[0];
                if cameraX != 0 &amp;&amp; cameraX != room_width - view_wview[0]
                {
                    //background control
                    background_x[2] -= obj_earthController.bgSpeed1;
                    background_x[1] -= obj_earthController.bgSpeed2;
                    background_x[0] -= obj_earthController.bgSpeed3;
                            
                    //object control
                    obj_earthTreeBG3.x += obj_earthController.fgSpeed1;
                }
            }
            else
            {
                x -= distance_to_object(testInt) - 1;
            }   
        }
        else if keyboard_check(ord("D"))    //LEFT MOVEMENT ---------------------------------------------------------------------------------------------------------
        {
            image_xscale = abs(image_xscale);
            testInt = instance_place(x + Speed, y, Solid);
            var cameraX = view_xview[0];
            var cameraEnd = room_width - view_wview[0]
            
            if testInt == noone
            {
                x += Speed;
                    
                //parallax effect right
                if (cameraX != cameraEnd &amp;&amp; cameraX != 0)
                {
                    //background control
                    background_x[2] += obj_earthController.bgSpeed1;
                    background_x[1] += obj_earthController.bgSpeed2;
                    background_x[0] += obj_earthController.bgSpeed3;
                            
                    //object control
                    obj_earthTreeBG3.x -= obj_earthController.fgSpeed1;
                }
            }
            else
            {
                x += distance_to_object(testInt) - 1;
            }  
        }
    }
}
</string>
