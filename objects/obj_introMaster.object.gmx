        <arguments>
          <argument>
            <kind>1</kind>
            <string>///control fade in and out of text
if textFadeIn
{
    if transition
    {
        obj_choice1.image_alpha += .01;
        obj_choice2.image_alpha += .01;
        obj_choice3.image_alpha += .01;
        if choice == 2 || choice == 3
        {
            obj_choice4.image_alpha += .01;
        }
    }
    obj_flavorText.image_alpha += .01;
    
    if obj_flavorText.image_alpha &gt; 1
    {
        textFadeIn = false;
    }
}
else if textFadeOut
{
    if transition
    {
        obj_choice1.image_alpha -= .01;
        obj_choice2.image_alpha -= .01;
        obj_choice3.image_alpha -= .01;
        if choice == 2 || choice == 3
        {
            obj_choice4.image_alpha -= .01;
        }
    }
    
    obj_flavorText.image_alpha -= .01;
    
    if obj_flavorText.image_alpha &lt; 0
    {
        if transition
        {
            choice++;
            instance_destroy(obj_choice1);
            instance_destroy(obj_choice2);
            instance_destroy(obj_choice3);
            if choice == 2 || choice == 3
            {
                instance_destroy(obj_choice4);
            }
        }
        
        textFadeOut = false;
        instance_destroy(obj_flavorText);
        alarm[0] = room_speed * 2;
    }
}
</string>
